{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "../",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "baseUrl": "..",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@engine/*": [
        "src/engine/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@logic/*": [
        "src/engine/logic/*"
      ],
      "@ai/*": [
        "src/engine/ai/*"
      ],
      "@setup/*": [
        "src/engine/setup/*"
      ],
      "@engineConfigs/*": [
        "src/engine/configs/*"
      ],
      "@engineTypes/*": [
        "src/engine/types/*"
      ],
      "@bionics/*": [
        "src/shared/components/*"
      ],
      "@bionics": [
        "src/shared/components"
      ],
      "@atoms/*": [
        "src/shared/components/atoms/*"
      ],
      "@atoms": [
        "src/shared/components/atoms"
      ],
      "@molecules/*": [
        "src/shared/components/molecules/*"
      ],
      "@molecules": [
        "src/shared/components/molecules"
      ],
      "@organisms/*": [
        "src/shared/components/organisms/*"
      ],
      "@organisms": [
        "src/shared/components/organisms"
      ],
      "@templates/*": [
        "src/shared/components/templates/*"
      ],
      "@templates": [
        "src/shared/components/templates"
      ],
      "@hooks/*": [
        "src/shared/hooks/*"
      ],
      "@hooks": [
        "src/shared/hooks"
      ],
      "@utils/*": [
        "src/shared/utils/*"
      ],
      "@utils": [
        "src/shared/utils"
      ],
      "@sharedTypes/*": [
        "src/shared/types/*"
      ],
      "@assets/*": [
        "src/app/assets/*"
      ],
      "@game/*": [
        "src/app/routes/game/*"
      ],
      "@menu/*": [
        "src/app/routes/menu/*"
      ],
      "@routes/*": [
        "src/app/routes/*"
      ],
      "@routes": [
        "src/app/routes"
      ],
      "@constants/*": [
        "src/constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}